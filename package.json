{
  "name": "scrabble-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "db:drop": "dropdb scrabble_express_dev; dropdb scrabble_express_test",
    "db:create": "createdb scrabble_express_dev; createdb scrabble_express_test",
    "db:schema": "node tasks/load_schema.js",
    "db:seed": "node tasks/seed.js",
    "db:reset": "npm run db:drop; npm run db:create; npm run db:schema; npm run db:seed",
    "test": "NODE_ENV=test ./node_modules/.bin/jasmine-node --verbose spec/"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "jasmine-node": "^1.14.5",
    "massive": "^2.3.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.9.2"
  }
}
